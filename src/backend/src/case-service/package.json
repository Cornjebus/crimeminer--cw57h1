{
  "name": "@crimeminer/case-service",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --sourceMap false",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "test:security": "jest --testMatch='**/*.security.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "security-scan": "snyk test",
    "audit": "npm audit",
    "prepare": "npm run security-scan && npm run build",
    "validate": "npm run lint && npm run test:coverage && npm run security-scan"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "winston": "^3.8.2",
    "joi": "^17.9.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.9.0",
    "express-validator": "^7.0.0",
    "express-correlation-id": "^2.0.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "@aws-sdk/client-kms": "^3.0.0",
    "keycloak-connect": "^21.0.0",
    "ioredis": "^5.3.2",
    "@elastic/elasticsearch": "^8.9.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/express": "^4.17.17",
    "@types/keycloak-connect": "^8.0.1",
    "eslint": "^8.40.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "prettier": "^2.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "snyk": "^1.1150.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^8.0.0",
    "ioredis-mock": "^8.2.2"
  }
}