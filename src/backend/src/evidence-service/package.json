{
  "name": "@crimeminer/evidence-service",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc -b --incremental",
    "start": "node --max-old-space-size=8192 dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov",
    "test:security": "npm audit && snyk test",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist coverage",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.0.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@elastic/elasticsearch": "8.9.0",
    "@elastic/elasticsearch-security": "8.9.0",
    "aws-sdk": "^2.1450.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-correlation-id": "^2.0.0",
    "express-rate-limit": "6.9.0",
    "express-validator": "7.0.1",
    "ffmpeg-static": "^5.2.0",
    "helmet": "7.0.0",
    "http-proxy-middleware": "2.0.6",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "ioredis": "5.3.2",
    "kafkajs": "^2.2.4",
    "keycloak-connect": "21.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@faker-js/faker": "8.0.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/keycloak-connect": "^8.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "aws-sdk-mock": "^5.8.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "ioredis-mock": "8.2.2",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "mock-aws-s3": "4.0.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "snyk": "^1.1190.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}