{
  "name": "crimeminer-web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "npm audit && snyk test",
    "lint": "eslint src --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "typecheck": "tsc --noEmit --strict",
    "analyze": "source-map-explorer 'dist/**/*.js'",
    "audit:deps": "npm audit && depcheck",
    "audit:licenses": "license-checker --production --onlyAllow \"MIT;Apache-2.0;BSD-3-Clause\"",
    "cspell": "cspell \"src/**/*.{ts,tsx,scss}\"",
    "validate": "npm run typecheck && npm run lint && npm run test && npm run audit:deps"
  },
  "dependencies": {
    "@azure/msal-react": "^2.0.0",
    "@elastic/apm-rum-react": "^2.0.0",
    "@emotion/react": "^11.11.0",
    "@mui/material": "^5.0.0",
    "@mui/x-date-pickers": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react": "^7.0.0",
    "@tanstack/react-virtual": "^3.0.0",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "crypto-js": "4.1.1",
    "d3-zoom": "^3.0.0",
    "date-fns": "^2.30.0",
    "filesize": "^10.0.0",
    "focus-trap-react": "^10.0.0",
    "i18next": "^23.0.0",
    "i18next-browser-languagedetector": "^7.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-force-graph": "^1.43.0",
    "react-hook-form": "^7.0.0",
    "react-i18next": "^13.0.0",
    "react-idle-timer": "^5.0.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
    "react-virtual": "^2.10.4",
    "redux-logger": "^3.0.6",
    "redux-persist-transform-encrypt": "^3.0.1",
    "redux-thunk": "^2.4.2",
    "resilience4j-ts": "^1.0.0",
    "sanitize-html": "^2.11.0",
    "socket.io-client": "^4.7.2",
    "vis-timeline": "^7.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.7.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@njpatel/clamav.js": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-responsive": "^9.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cspell": "^6.31.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.42.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.6.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.6.0",
    "license-checker": "^25.0.1",
    "msw": "^1.2.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.63.0",
    "snyk": "^1.1144.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.0",
    "whatwg-fetch": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}